{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "src/App.tsx",
    "vite.config.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/node_modules/**",
    "dist/**",
    "build/**"
  ],
  "ignoreDependencies": [
    "@types/*"
  ]
}
